	border: none;
	padding: $base;
	color: $black;
	text-align: center;
	@include transition;
	&:hover, &:focus { 
		background: darken($black, 3);
		color: #fff;
	}
}

.btn-alt { 
	margin-bottom: 0;

	a { 
		@extend .btn;
		background: $black-light;
		color: #fff;
		text-decoration: none;
		display: block;
		margin-bottom: 0;
	}
}
