/*-------------------------------------------

   	Forms

-------------------------------------------*/

label, .form-heading {
	font-weight: normal;
	font-size: 20px;
	margin-bottom: $base/2;
	display: block;
}

.form-heading + .inline { margin-top: $base/2; }

ul.inline, p.inline {
	display: block;
