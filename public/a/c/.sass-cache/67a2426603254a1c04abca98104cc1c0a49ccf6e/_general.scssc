	@media (max-width: 700px) { 
		display: none;
	}
}

.block-caption-more {
	background: #fff; 
	padding: 0 $base*1.5;
	overflow: hidden;
    max-height: 0;
    -webkit-transition: all 0.3s linear;
    -moz-transition: all 0.3s linear;
    -o-transition: all 0.3s linear;
    transition: all 0.3s linear;
    .meta { 
    	margin-top: 18px;
    	@extend %h5;
    	color: $black-lightest;
    	margin-bottom: $base*2.5;

    	span { @extend %h6; } 
